//Bruteforce Approach

//Bruteforce Approach

class Solution {
    public String smallestNumber(int sum, int d) {
        long maxDDigitNumber = maximumDDigitNumber(d);

        if (sum > sumOfDigits(maxDDigitNumber)) {
            return "-1"; // Not possible to achieve the sum
        }

        long num = minimumDDigitNumber(d);

        while (num <= maxDDigitNumber) {
            if (sumOfDigits(num) == sum) {
                break;
            }

            num++;
        }

        return Long.toString(num);
    }

    private long sumOfDigits(long num) {
        long sum = 0;

        while (num != 0) {
            sum += (num % 10);
            num = num / 10;
        }

        return sum;
    }

    private long maximumDDigitNumber(int d) {
        return (int) Math.pow(10, d) - 1;
    }

    private long minimumDDigitNumber(int d) {
        if (d == 1) {
            return 0;
        }

        return (int) Math.pow(10, d - 1);
    }
}


//Improved Approach

class Solution {
    public String smallestNumber(int sum, int d) {
        if (sum > (9 * d)) {
            return "-1";
        }

        long maxDDigitNumber = maximumDDigitNumber(d);
        long num = minimumDDigitNumber(d);

        while (num <= maxDDigitNumber) {
            if (sumOfDigits(num) == sum) {
                break;
            }

            num++;
        }

        return Long.toString(num);
    }

    private long sumOfDigits(long num) {
        long sum = 0;

        while (num != 0) {
            sum += (num % 10);
            num = num / 10;
        }

        return sum;
    }

    private long maximumDDigitNumber(int d) {
        return (int) Math.pow(10, d) - 1;
    }

    private long minimumDDigitNumber(int d) {
        if (d == 1) {
            return 0;
        }

        return (int) Math.pow(10, d - 1);
    }
}


//Optimal Approach

/*
  0 is the Only Number(A 1 digit Number) whose Sum of digits is 0,
  there exists no Number more than 1 digit whose sum of Digits is 0 so this is the Corner case which is required to be handled Seprately.

-----------------------------------------------------------------------------------------------------------------------------------------------------------------------
MAXIMUM SUM OF DIGITS OBTAINED/POSSIBLE USING 1 DIGIT NUMBER = SUM_OF_DIGITS(MAX. 1 DIGIT NUMBER) = SUM_OF_DIGITS(9)=(9)=9.

